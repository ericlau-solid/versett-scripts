{
  "name": "versett-scripts",
  "version": "0.0.0-semantically-released",
  "description": "Collection of scripts that are useful for V// projects.",
  "main": "index.js",
  "scripts": {
    "commitmsg": "versett-devtools commitmsg ${GIT_PARAMS}",
    "precommit": "versett-devtools precommit",
    "preparecommitmsg": "versett-devtools preparecommitmsg ${GIT_PARAMS}",
    "release": "yarn && versett-devtools release",
    "test": "echo \"No tests were specified! Please write some tests.\"",
    "test:ci": "yarn release --dry-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/versett/versett-scripts.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/versett/versett-scripts/issues"
  },
  "homepage": "https://github.com/versett/versett-scripts#readme",
  "devDependencies": {
    "@versett/versett-devtools": "^1.11.1"
  },
  "lint-staged": {
    "*.{js,json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "release": {
    "getLastRelease": "last-release-git"
  },
  "dependencies": {}
}
