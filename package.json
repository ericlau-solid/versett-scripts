{
  "name": "@versett/versett-scripts",
  "version": "0.0.0-semantically-released",
  "description": "Collection of scripts that are useful for V// projects.",
  "main": "index.js",
  "scripts": {
    "commitmsg": "versett-devtools commitmsg ${GIT_PARAMS}",
    "precommit": "versett-devtools precommit",
    "preparecommitmsg": "versett-devtools preparecommitmsg ${GIT_PARAMS}",
    "release": "yarn && versett-devtools release --npm-publish",
    "test": "echo \"No tests were specified! Please write some tests.\"",
    "test:ci": "echo \"No tests were specified! Please write some tests.\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/versett/versett-scripts.git"
  },
  "author": "",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/versett/versett-scripts/issues"
  },
  "homepage": "https://github.com/versett/versett-scripts#readme",
  "lint-staged": {
    "*.{js,json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "release": {
    "getLastRelease": "last-release-git"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "fs-extra": "^3.0.1",
    "husky": "^0.13.4",
    "lint-staged": "^8.0.4",
    "lodash.merge": "^4.6.0",
    "prettier": "^1.7.0",
    "sort-object": "^3.0.2",
    "url-loader": "^1.0.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@versett/versett-devtools": "^1.11.1",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-prettier": "^2.6.0"
  }
}
